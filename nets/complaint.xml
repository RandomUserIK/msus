		    <id>9_finish</id>
		    <actions phase="pre">
					<action id="100">
					<!-- @formatter:off -->
					parent_id: f.parent_id;

					def parentCase = workflowService.findOne(parent_id.value)
					parentCase.dataSet['claim_id'].value = ""
					workflowService.save(parentCase)

					String reklamaciaId = getTaskId("70", parentCase)
					taskService.assignTask(reklamaciaId)
